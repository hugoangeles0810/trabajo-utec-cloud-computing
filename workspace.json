          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["services/product-service/**/*.{ts,js}"]
          }
        },
        "deploy": {
          "executor": "@nx/run-commands:run-commands",
          "options": {
            "command": "cd services/product-service && npm run deploy"
          }
        }
      },
      "tags": ["type:app", "scope:services"]
    },
