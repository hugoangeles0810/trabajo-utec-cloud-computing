{
  "version": 2,
  "projects": {
    "gamarriando-frontend": {
      "root": "frontend",
      "sourceRoot": "frontend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nx/next:build",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "outputPath": "dist/frontend"
          },
          "configurations": {
            "development": {
              "outputPath": "dist/frontend"
            },
            "production": {}
          }
        },
        "serve": {
          "executor": "@nx/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "gamarriando-frontend:build",
            "dev": true
          },
          "configurations": {
            "development": {
              "buildTarget": "gamarriando-frontend:build:development",
              "dev": true
            },
            "production": {
              "buildTarget": "gamarriando-frontend:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nx/next:export",
          "options": {
            "buildTarget": "gamarriando-frontend:build:production"
          }
        },
        "test": {
          "executor": "@nx/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
          "options": {
            "jestConfig": "frontend/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nx/eslint:lint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["frontend/**/*.{ts,tsx,js,jsx}"]
          }
        }
      },
      "tags": ["type:app", "scope:frontend"]
    },
    "product-service": {
      "root": "services/product-service",
      "sourceRoot": "services/product-service/app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nx/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/services/product-service",
            "main": "services/product-service/main.py",
            "tsConfig": "services/product-service/tsconfig.json"
          }
        },
        "serve": {
          "executor": "@nx/node:serve",
          "options": {
            "buildTarget": "product-service:build"
